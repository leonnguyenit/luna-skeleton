{
    "name": "leonnguyenit/luna-skeleton",
    "description": "A simple app skeleton build on Slim Framework",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Leon Nguyen IT",
            "email": "thanhluan12a14@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "slim/slim": "^3.0",
        "twig/twig": "^1.28",
        "illuminate/events": "^5.2",
        "illuminate/database": "^5.2",
        "illuminate/pagination": "^5.2"
    },
    "require-dev": {
        "raveren/kint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Luna\\": [
                "app/src",
                "app/src/Libraries"
            ]
        },
        "files": [
            "app/src/Support/helpers.php"
        ]
    }
}
